<?xml version='1.0' encoding='utf-8'?>
<widget id="com.amanganiello90.angular" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>angularDashFullStack</name>
    <description>
        A full stack angular app.
    </description>
    <author email="angelo.mang@libero.it" href="https://github.com/amanganiello90/java-angular-web-app">
        amanganiello90
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-httpd" spec="^0.9.3" />
    <engine name="android" spec="^7.0.0" />
</widget>
