{
  "name": "electron-app",
  "description": "Demonstrates spawning Express from Electron and using it to serve content or create an exe only for angular build",
  "version": "1.0.0",
  "main": "main.js",
  "authors": [
    "Angelo Manganiello <angelo.mang@libero.it>"
  ],
  "license": "GPL-3.0",
  "scripts": {
	"preconfig": "xcopy  \"../frontend-app/dist\"  \"dist\"  /s /Y /r /i /H /O ", 
    "start": "npm run preconfig && electron .",
    "package": "npm run preconfig && electron-packager . --overwrite --platform=win32 --arch=ia32 --prune=true --out=distribution --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron-exe-With-Express\""
  },
  "dependencies": {
    "electron": "1.8.2",
    "electron-packager": "11.1.0",
    "express": "4.16.2",
    "jquery": "3.3.1",
    "keymaster": "1.6.2",
    "lodash": "4.17.4",
    "request": "2.83.0"
  }
}
